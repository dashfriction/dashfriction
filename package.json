{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "c8": "^7.11.2",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.12.6"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {}
}
