{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "tf:init": "terraform init -chdir apps/terraform",
    "tf:dep": "terraform deploy ./apps/terraform",
    "tf:des": "terrform destroy -chdir ./apps/terraform"
  },
  "devDependencies": {
    "c8": "^7.11.2",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.12.6"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {
    "@mantine/core": "^4.2.4",
    "@mantine/dates": "^4.2.4",
    "@mantine/dropzone": "^4.2.4",
    "@mantine/form": "^4.2.4",
    "@mantine/hooks": "^4.2.4",
    "@mantine/modals": "^4.2.4",
    "@mantine/notifications": "^4.2.4",
    "@mantine/prism": "^4.2.4",
    "@mantine/rte": "^4.2.4",
    "@mantine/spotlight": "^4.2.4",
    "dayjs": "^1.11.2"
  }
}
